{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminUsername": {
      "value": "admin"
    },
    "vmName": {
      "value": "Nginx"
    },
    "artifactsPath": {
      "value": "https://raw.githubusercontent.com/juliosene/azure-nginx-php/master"
    },
    "customScriptCommandToExecute": {
      "value": "bash nginx-php-install.sh"
    },
    "dnsNameForPublicIP": {
      "value": "yourdnsname"
    },
    "nodeCount": {
      "value": 2
    }
  }
}
